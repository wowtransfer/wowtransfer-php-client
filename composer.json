{
    "name": "wowtransfer/chdphp",
    "description": "WoW transfer client for characters creating",
    "keywords": ["wow", "transfer", "create character", "lua dump", "lua to sql", "character transfer"],
    "homepage": "http://wowtransfer.com/",
    "type": "project",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/wowtransfer/chdphp/issues?state=open",
        "forum": "http://forum.wowtransfer.com/",
        "wiki": "http://wowtransfer.loc/docs/",
        "source": "https://github.com/wowtransfer/chdphp"
    },
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.5.0",
        "yiisoft/yii": "1.1.17",
        "twbs/bootstrap": "3.3.6",
        "wowtransfer/php-sdk": "master",
        "requirejs/requirejs": "*",
        "requirejs/r.js": "*"
    },
    "require-dev": {
        "phing/phing": "*"
    },
    "config": {
        "process-timeout": 1800,
        "vendor-dir": "public_html/protected/vendor",
        "bin-dir": "public_html/protected/vendor/bin",
        "secure-http": false
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "requirejs/requirejs",
                "version": "2.2.0",
                "dist": {
                    "url": "http://requirejs.org/docs/release/2.2.0/minified/require.js",
                    "type": "file"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "requirejs/r.js",
                "version": "2.2.0",
                "dist": {
                    "url": "http://requirejs.org/docs/release/2.2.0/r.js",
                    "type": "file"
                }
            }
        }
    ]
}
